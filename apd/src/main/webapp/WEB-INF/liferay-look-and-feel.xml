<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>7.2.0+</version>
	</compatibility>
	<theme id="apd" name="APD">
		<template-extension>ftl</template-extension>
		<settings>
	    	<setting type="select" configurable="true" 
	        	key="theme_type" 
	    	    options="home,page"                   
		        value="page">
		    </setting>
		    <setting configurable="true" key="m_address" type="text" value=""></setting>
		    <setting configurable="true" key="m_phone" type="text" value=""></setting>
		    <setting configurable="true" key="m_fax" type="text" value=""></setting>
        </settings>
		<page-theme>true</page-theme>
        <color-scheme id="page-chung" name="Chung">
			<default-cs>true</default-cs>
			<css-class>style-chung</css-class>
			<color-scheme-images-path>
				${images-path}/custom-images/color-schemes/${css-class}
			</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="phong-tchc" name="P.Tổ chức Hành chính">
